{
  "name": "infotech/reference",
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": "^2.0",
    "ext-mbstring": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "infotech\\reference\\": "src"
    }
  },
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^6.0",
    "yiisoft/yii2-coding-standards": "^2.0",
    "squizlabs/php_codesniffer": "^3.4"
  }
}
