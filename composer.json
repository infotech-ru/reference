{
  "name": "infotech/reference",
  "require": {
    "php": ">=7.4",
    "ext-mbstring": "*",
    "ext-json": "*",
    "yiisoft/yii2": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "infotech\\reference\\": "src",
      "infotech\\reference\\tests\\": "tests"
    }
  },
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.0",
    "squizlabs/php_codesniffer": "^3.4",
    "phpstan/phpstan": "^1.10"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
